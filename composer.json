        "voku/portable-utf8": "^6",
        "symfony/dependency-injection": "^6.0",
        "symfony/mailer": "^6.0",
        "nikic/php-parser": "^4.13",
        "symfony/console": "^6.0",
        "nelexa/zip": "^4.0",
        "symfony/finder": "^6.0",
        "symfony/process": "^6.0",
        "symfony/error-handler": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/twig-bundle": "^6.0"
